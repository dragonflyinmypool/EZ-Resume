<form action="/add-info/add-education" method="POST">

  <label for="school">School:</label><br>
  <input type="text" id="school" name="school" required><br>

  <label for="degree">Degree:</label><br>
  <input type="text" id="degree" name="degree" required><br>

  <label for="fieldOfStudy">Field Of Study:</label><br>
  <input type="text" id="fieldOfStudy" name="fieldOfStudy" required><br>

  <label for="start-date">Start Date:</label><br>
  <input type="month" id="start-date" name="startDate" required><br>

  <label for="end-date">End Date:</label><br>
  <input type="month" id="end-date" name="endDate" required><br>

  <button type="submit">Add</button>
</form>